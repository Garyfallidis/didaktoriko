issn = {1558-0062},
journal = {IEEE transactions on medical imaging},
keywords = {Algorithms,Brain,Brain: anatomy \& histology,Cluster Analysis,Diffusion Tensor Imaging,Diffusion Tensor Imaging: methods,Humans,Image Processing, Computer-Assisted,Image Processing, Computer-Assisted: methods,Models, Neurological,Normal Distribution,Reproducibility of Results},
month = jan,
