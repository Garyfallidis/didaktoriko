@article{Ding2003a,
abstract = {Quantitative characterization of neuronal fiber pathways in vivo is of significant neurological and clinical interest. Using the capability of MR diffusion tensor imaging to determine the local orientations of neuronal fibers, novel algorithms were developed to bundle neuronal fiber pathways reconstructed in vivo with diffusion tensor images and to quantify various physical and geometric properties of fiber bundles. The reliability of the algorithms was examined with reproducibility tests. Illustrative results show that consistent physical and geometric measurements of novel properties of neuronal tissue can be obtained, which offer considerable potential for the quantitative study of fiber pathways in vivo.},
