journal = {Radiology},
keywords = {Adult,Age Factors,Brain,Brain: anatomy \& histology,Diffusion Magnetic Resonance Imaging,Diffusion Magnetic Resonance Imaging: instrumentat,Humans,Infant, Newborn,ROC Curve},
month = dec,
number = {3},
pages = {673--81},
pmid = {14657305},
title = {{Comparisons of regional white matter diffusion in healthy neonates and adults performed with a 3.0-T head-only MR imaging unit.}},
