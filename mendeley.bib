keywords = {Adult,Basal Ganglia,Basal Ganglia: physiology,Brain Mapping,Empathy,Female,Games, Experimental,Gyrus Cinguli,Gyrus Cinguli: physiology,Humans,Magnetic Resonance Imaging,Male,Prefrontal Cortex,Prefrontal Cortex: physiology,Reward,Self Concept,Social Behavior,Social Desirability,Young Adult},
