	SB - IM},
  keywords = {Algorithms, Diffusion, Diffusion Magnetic Resonance Imaging, Magnetic
	Resonance Imaging},
  owner = {lester},
  refid = {139},
  timestamp = {2009.01.12},
  url = {PM:16394152}
}

@ARTICLE{,
  author = {Alexander, D.C. and Barker, G.J.},
